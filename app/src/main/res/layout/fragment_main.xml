<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity$PlaceholderFragment">

    <TextView
        android:layout_width="280dp"
        android:layout_height="63dp"
        android:id="@+id/editText"
        android:layout_x="15dp"
        android:layout_y="25dp"
        android:background="#ff9bfcff"
        android:backgroundTintMode="add"
        android:editable="true"
        android:inputType="textNoSuggestions"
        android:textAlignment="gravity"
        android:text="hello"
        android:gravity="center_vertical|right|center_horizontal"
        android:textSize="40dp"
        android:theme="@style/Base.TextAppearance.AppCompat.Display2"
        android:typeface="normal"
        android:enabled="true"
        android:focusable="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="C"
        android:textSize="25dp"
        android:id="@+id/button1"
        android:layout_x="20dp"
        android:layout_y="120dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="/"
        android:textSize="25dp"
        android:id="@+id/button2"
        android:layout_x="90dp"
        android:layout_y="120dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:textSize="25dp"
        android:id="@+id/button3"
        android:layout_x="160dp"
        android:layout_y="120dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="Clr"
        android:textSize="22.2dp"
        android:id="@+id/button4"
        android:layout_x="230dp"
        android:layout_y="120dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="7"
        android:textSize="25dp"
        android:id="@+id/button5"
        android:layout_x="20dp"
        android:layout_y="180dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="8"
        android:textSize="25dp"
        android:id="@+id/button6"
        android:layout_x="90dp"
        android:layout_y="180dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="9"
        android:textSize="25dp"
        android:id="@+id/button7"
        android:layout_x="160dp"
        android:layout_y="180dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="-"
        android:textSize="25dp"
        android:id="@+id/button8"
        android:layout_x="230dp"
        android:layout_y="180dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="4"
        android:textSize="25dp"
        android:id="@+id/button9"
        android:layout_x="20dp"
        android:layout_y="240dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="5"
        android:textSize="25dp"
        android:id="@+id/button10"
        android:layout_x="90dp"
        android:layout_y="240dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="6"
        android:textSize="25dp"
        android:id="@+id/button11"
        android:layout_x="160dp"
        android:layout_y="240dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="+"
        android:textSize="25dp"
        android:id="@+id/button12"
        android:layout_x="230dp"
        android:layout_y="240dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="1"
        android:textSize="25dp"
        android:id="@+id/button13"
        android:layout_x="20dp"
        android:layout_y="300dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="2"
        android:textSize="25dp"
        android:id="@+id/button14"
        android:layout_x="90dp"
        android:layout_y="300dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="3"
        android:textSize="25dp"
        android:id="@+id/button15"
        android:layout_x="160dp"
        android:layout_y="300dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="( )"
        android:textSize="25dp"
        android:id="@+id/button16"
        android:layout_x="230dp"
        android:layout_y="300dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="25dp"
        android:id="@+id/button17"
        android:layout_x="20dp"
        android:layout_y="360dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="."
        android:textSize="25dp"
        android:id="@+id/button18"
        android:layout_x="90dp"
        android:layout_y="360dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="+/-"
        android:textSize="20.2dp"
        android:id="@+id/button19"
        android:layout_x="160dp"
        android:layout_y="360dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="="
        android:textSize="25dp"
        android:id="@+id/button20"
        android:layout_x="230dp"
        android:layout_y="360dp" />

</AbsoluteLayout>